<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_clubes"
        tools:context=".ClubesActivity" android:background="@color/colorFondo">

    <ImageView
            android:layout_width="368dp"
            android:layout_height="117dp" app:srcCompat="@drawable/logo_horizontal_blanco"
            android:id="@+id/imageView3"
            android:contentDescription="TODO"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:layout_marginBottom="450dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <ScrollView
            android:layout_width="0dp"
            android:layout_height="434dp" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:id="@+id/scrollView2"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0">

        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnDebates"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btnTeatro"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@drawable/debates" android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="253dp"
                    android:layout_marginRight="253dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnAjedrez"
                    app:layout_constraintTop_toBottomOf="@+id/btnBaloncesto"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="4dp" android:layout_marginStart="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/btnTT"
                    app:layout_constraintBottom_toTopOf="@+id/btnYoga"
                    android:background="@drawable/ajedrez" android:layout_marginTop="8dp"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnYoga"
                    app:layout_constraintTop_toBottomOf="@+id/btnAjedrez"
                    app:layout_constraintEnd_toStartOf="@+id/btnCoro"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="4dp" android:layout_marginStart="4dp"
                    app:layout_constraintBottom_toTopOf="@+id/btnTeatro" android:background="@drawable/yoga"
                    android:layout_marginTop="8dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnTeatro"
                    app:layout_constraintTop_toBottomOf="@+id/btnYoga" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="4dp" android:layout_marginStart="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/btnGuitarra"
                    app:layout_constraintBottom_toTopOf="@+id/btnDebates"
                    android:background="@drawable/teatro" android:layout_marginTop="8dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnGuitarra"
                    android:layout_marginLeft="14dp" android:layout_marginStart="14dp"
                    app:layout_constraintStart_toEndOf="@+id/btnTeatro"
                    app:layout_constraintTop_toBottomOf="@+id/btnCoro"
                    android:background="@drawable/guitarra"
                    android:layout_marginTop="8dp" app:layout_constraintEnd_toStartOf="@+id/btnDanza"
                    android:layout_marginEnd="12dp" android:layout_marginRight="12dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnDanza"
                    app:layout_constraintTop_toBottomOf="@+id/btnMarimba"
                    app:layout_constraintStart_toEndOf="@+id/btnGuitarra"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="5dp" android:layout_marginRight="5dp"
                    android:background="@drawable/danza" android:layout_marginTop="8dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnCoro"
                    app:layout_constraintTop_toBottomOf="@+id/btnTT" android:layout_marginLeft="14dp"
                    android:layout_marginStart="14dp" app:layout_constraintStart_toEndOf="@+id/btnYoga"
                    app:layout_constraintBottom_toTopOf="@+id/btnGuitarra"
                    android:background="@drawable/coro" android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/btnMarimba" android:layout_marginEnd="12dp"
                    android:layout_marginRight="12dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnMarimba"
                    app:layout_constraintStart_toEndOf="@+id/btnCoro"
                    app:layout_constraintTop_toBottomOf="@+id/btnAcondicionamiento" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="5dp" android:layout_marginRight="5dp"
                    app:layout_constraintBottom_toTopOf="@+id/btnDanza" android:background="@drawable/marimba"
                    android:layout_marginTop="8dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnTT"
                    app:layout_constraintTop_toBottomOf="@+id/btnFutbol" android:layout_marginLeft="14dp"
                    android:layout_marginStart="14dp" app:layout_constraintStart_toEndOf="@+id/btnAjedrez"
                    app:layout_constraintBottom_toTopOf="@+id/btnCoro" android:background="@drawable/tenisdemesa"
                    android:layout_marginTop="8dp" app:layout_constraintEnd_toStartOf="@+id/btnAcondicionamiento"
                    android:layout_marginEnd="12dp" android:layout_marginRight="12dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnAcondicionamiento"
                    app:layout_constraintTop_toBottomOf="@+id/btnVoleibol"
                    app:layout_constraintStart_toEndOf="@+id/btnTT"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp" app:layout_constraintBottom_toTopOf="@+id/btnMarimba"
                    android:background="@drawable/acondicionamientofi" android:layout_marginTop="8dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnBaloncesto"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="4dp" android:layout_marginStart="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/btnFutbol"
                    app:layout_constraintBottom_toTopOf="@+id/btnAjedrez"
                    android:background="@drawable/baloncesto" android:onClick="metodoBaloncesto"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnFutbol"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginLeft="14dp"
                    android:layout_marginStart="14dp" app:layout_constraintStart_toEndOf="@+id/btnBaloncesto"
                    app:layout_constraintBottom_toTopOf="@+id/btnTT" android:background="@drawable/futbol"
                    app:layout_constraintEnd_toStartOf="@+id/btnVoleibol" android:layout_marginEnd="12dp"
                    android:layout_marginRight="12dp"/>
            <Button
                    android:layout_width="111dp"
                    android:layout_height="108dp" android:id="@+id/btnVoleibol"
                    app:layout_constraintStart_toEndOf="@+id/btnFutbol" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/btnAcondicionamiento" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="5dp" android:layout_marginRight="5dp"
                    android:background="@drawable/voleibol"/>
        </android.support.constraint.ConstraintLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>